<head>
  <title>TEDU</title>
  <meta charset="UTF-8" />
  <link rel="shortcut icon" href="/Content/client/images/favicon.png" title="Favicon" />
  <meta name="viewport" content="width=device-width" />

  <link rel='stylesheet' id='magz-style-css' href='/Content/client/css/style.css' type='text/css' media='all' />
  <link rel='stylesheet' id='swipemenu-css' href='/Content/client/css/swipemenu.css' type='text/css' media='all' />
  <link rel='stylesheet' id='flexslider-css' href='/Content/client/css/flexslider.css' type='text/css' media='all' />
  <link rel='stylesheet' id='bootstrap-css' href='/Content/client/css/bootstrap.css' type='text/css' media='all' />
  <link rel='stylesheet' id='bootstrap-responsive-css' href='/Content/client/css/bootstrap-responsive.css' type='text/css' media='all' />
  <link rel='stylesheet' id='simplyscroll-css' href='/Content/client/css/jquery.simplyscroll.css' type='text/css' media='all' />
  <link rel='stylesheet' id='jPages-css' href='/Content/client/css/jPages.css' type='text/css' media='all' />
  <link rel='stylesheet' id='rating-css' href='/Content/client/css/jquery.rating.css' type='text/css' media='all' />
  <link rel='stylesheet' id='ie-styles-css' href='/Content/client/css/ie.css' type='text/css' media='all' />
  <link rel='stylesheet' id='Roboto-css' href='http://fonts.googleapis.com/css?family=Roboto' type='text/css' media='all' />

  <script type='text/javascript' src="/Content/client/js/jquery-1.10.2.min.js"></script>
  <script type='text/javascript' src='/Content/client/js/html5.js'></script>
  <script type='text/javascript' src='/Content/client/js/bootstrap.min.js'></script>
  <script type='text/javascript' src='/Content/client/js/jquery.flexslider.js'></script>
  <script type='text/javascript' src='/Content/client/js/jquery.flexslider.init.js'></script>
  <script type='text/javascript' src='/Content/client/js/jquery.bxslider.js'></script>
  <script type='text/javascript' src='/Content/client/js/jquery.bxslider.init.js'></script>
  <script type='text/javascript' src='/Content/client/js/jquery.rating.js'></script>
  <script type='text/javascript' src='/Content/client/js/jquery.idTabs.min.js'></script>
  <script type='text/javascript' src='/Content/client/js/jquery.simplyscroll.js'></script>
  <script type='text/javascript' src='/Content/client/js/fluidvids.min.js'></script>
  <script type='text/javascript' src='/Content/client/js/jPages.js'></script>
  <script type='text/javascript' src='/Content/client/js/jquery.sidr.min.js'></script>
  <script type='text/javascript' src='/Content/client/js/jquery.touchSwipe.min.js'></script>
  <script type='text/javascript' src='/Content/client/js/custom.js'></script>
  <!-- END -->
</head>
<body>
  <div id="page">
    <header id="header" class="container">
      <div id="mast-head">
        <div id="logo">
          <a href="/" title="Magazine" rel="home">
            <img src="/Content/client/images/logo.png" alt="Magazine" />
          </a>
        </div>
      </div>
      @{Html.RenderAction("MainMenu", "Home");}

    </header><!-- #masthead -->

    @{Html.RenderAction("BreakingNews", "Home");}



    <div id="content" class="container">

      <div id="main" class="row-fluid">
        <div id="main-left" class="span8">

          @RenderBody()

        </div><!-- #main-left -->

        <div id="sidebar" class="span4">
          @{Html.RenderAction("NewsTab", "Home");}


          @{Html.RenderAction("RecentPost", "Home");}

          @*@{Html.RenderAction("SpecialBox", "Home");}*@

          @{Html.RenderAction("VideoGallery", "Home");}

        </div><!-- sidebar -->

        <div class="clearfix"></div>
        @*@{Html.RenderAction("Gallery", "Home");}*@
      </div><!-- #main -->

    </div><!-- #content -->

    <footer id="footer" class="row-fluid">
      @{Html.RenderAction("Footer", "Home");}
    </footer>

  </div><!-- #wrapper -->

</body>
</html>
